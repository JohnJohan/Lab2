// Concrete Strategy: Warrior
public class WarriorRole implements RoleStrategy {
    @Override
    public void attack() {
        System.out.println("Warrior attacks with a sword!");
    }

    @Override
    public void defend() {
        System.out.println("Warrior defends with a shield!");
    }
}

// Concrete Strategy: Mage
public class MageRole implements RoleStrategy {
    @Override
    public void attack() {
        System.out.println("Mage casts a fireball!");
    }

    @Override
    public void defend() {
        System.out.println("Mage creates a magic barrier!");
    }
}

// Concrete Strategy: Archer
public class ArcherRole implements RoleStrategy {
    @Override
    public void attack() {
        System.out.println("Archer shoots an arrow!");
    }

    @Override
    public void defend() {
        System.out.println("Archer dodges the attack!");
    }
}
